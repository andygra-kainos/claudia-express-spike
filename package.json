{
  "name": "claudia-express-spike",
  "description": "Example of a vanilla Express app wrapped for serverless deployment to AWS lambda",
  "author": "Andy Gray <https://github.com/andygra-kainos>",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:andygra-kainos/claudia-express-spike.git"
  },
  "scripts": {
    "start": "node ./bin/www",
    "claudia:install": "npm install --save-dev claudia",
    "claudia:generate": "./node_modules/.bin/claudia generate-serverless-express-proxy --express-module app",
    "claudia:create": "./node_modules/.bin/claudia create --profile claudia --handler lambda.handler --deploy-proxy-api --region eu-west-1",
    "claudia:pack": "./node_modules/.bin/claudia update --profile claudia",
    "claudia:test": "./node_modules/.bin/claudia test-lambda --profile claudia",
    "claudia:update": "./node_modules/.bin/claudia update --profile claudia"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "twig": "~0.10.3"
  },
  "devDependencies": {
    "claudia": "^5.1.2"
  }
}
